{
    "name": "gos/mailer-bundle",
    "description": "Mailer Bundle, easy email templating & reusable",
    "keywords": ["mailer", "send", "Bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require":
    {
        "php": ">=5.4",
        "sensio/framework-extra-bundle": "~3.0",
        "gos/doctrine-bundle": "~1.0@dev"
    },
    "autoload": {
        "psr-4": { "Gos\\Bundle\\MailerBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Bundle\\MailerBundle\\Tests\\": "/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "minimum-stability": "dev"
    }
}
